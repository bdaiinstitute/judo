  <include file="spot_components/params_and_default_locomotion.xml" />
  <include file="spot_components/assets_locomotion.xml" />

  <worldbody>
    <geom name="ground" type="plane" size="10 10 0.01" friction="0.8" priority="5" material="blue_grid"/>

    <body name="body" pos="0 0 0.7">
      <joint name="base" type="free"/>
      <inertial mass="16.70765207" pos="0 0 -0.00496172" fullinertia="0.081543792 0.549231154 0.569454373 0 0 0"/>
      <geom name="body_0_visual" class="visual" mesh="body_0_visual" material="black"/>
      <geom name="body_1_visual" class="visual" mesh="body_1_visual" material="yellow"/>
      <geom name="body" class="collision" type="box" size="0.42 0.11 0.08"/>
      <site name="site_body" pos="0 0 0.3" size="0.01"/>

      <include file="spot_components/legs_locomotion.xml" />
      <include file="spot_components/arm_locomotion.xml" />
    </body>
    <body name="box" pos="0 0 0.275">
    <joint name="box_joint" type="free"/>
    <inertial pos="0 0 0" mass="1.5" diaginertia="0.01 0.01 0.01"/>
    <geom name="c1" pos="0.275 0.275 0.275" class="corner_contact"/>
    <geom name="c2" pos="-0.275 0.275 0.275" class="corner_contact"/>
    <geom name="c3" pos="0.275 -0.275 0.275" class="corner_contact"/>
    <geom name="c4" pos="-0.275 -0.275 0.275" class="corner_contact"/>
    <geom name="c5" pos="0.275 0.275 -0.275" class="corner_contact"/>
    <geom name="c6" pos="-0.275 0.275 -0.275" class="corner_contact"/>
    <geom name="c7" pos="0.275 -0.275 -0.275" class="corner_contact"/>
    <geom name="c8" pos="-0.275 -0.275 -0.275" class="corner_contact"/>
    <geom name="box_contact" type="box" size="0.275 0.275 0.275" class="collision"/>
    <site name="site_object" pos="0 0 0" size="0.01"/>
  </body>
  </worldbody>

  <include file="spot_components/actuators_sensors_position_control.xml" />
  <include file="spot_components/contact.xml" />
  
  <sensor>
    <!-- Object orientation sensor - measures Y axis of box relative to world -->
    <frameyaxis name="object_y_axis" objtype="body" objname="box" />
    <!-- End effector to object position sensor -->
    <framepos name="sensor_arm_link_fngr" objtype="site" objname="site_arm_link_fngr" reftype="site" refname="site_object" />
  </sensor>

</mujoco>